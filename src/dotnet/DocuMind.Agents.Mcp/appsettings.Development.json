{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Information",
            "Microsoft.Extensions.ModelContextProtocol": "Trace"
        }
    },
    "ModelContextProtocol": {
        "Security": {
            "EnableAuthentication": false,
            "AllowedOrigins": [
                "http://localhost:*",
                "https://localhost:*"
            ],
            "RateLimiting": {
                "RequestsPerMinute": 120,
                "RequestsPerHour": 2000
            }
        }
    }
}