{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Documind.Vision": "Debug"
    }
  },
  "AllowedHosts": "*",
  "VisionSettings": {
    "MaxFileSizeMB": 10,
    "SupportedFormats": [
      "jpg",
      "jpeg",
      "png",
      "bmp",
      "gif",
      "tiff",
      "webp"
    ],
    "ProcessingTimeoutSeconds": 30
  }
}